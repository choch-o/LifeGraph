<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    android:versionCode="1"
    android:versionName="1.0"
    package="com.twodevs.chocho.lifegraphKEDI" >

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >

        <activity
            android:name="com.twodevs.chocho.lifegraphKEDI.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.twodevs.chocho.lifegraphKEDI.GraphActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.twodevs.chocho.lifegraphKEDI.EventActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.twodevs.chocho.lifegraphKEDI.ListActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.twodevs.chocho.lifegraphKEDI.CategoryListActivity"
            android:label="@string/app_name" >
        </activity>
    </application>

</manifest>
